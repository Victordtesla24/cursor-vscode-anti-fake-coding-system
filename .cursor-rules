{
  "codeStyle": {
    "preferredFrameworks": ["react", "next.js"],
    "preferredLanguages": ["typescript", "javascript"],
    "indentationStyle": "spaces",
    "indentationSize": 2,
    "maxLineLength": 80
  },
  "aiGeneration": {
    "prohibitedPatterns": [
      "TODO",
      "FIXME",
      "console.log",
      "mock",
      "dummy",
      "fake"
    ],
    "enforcedPatterns": [
      "strict type checking",
      "error handling",
      "input validation"
    ],
    "codeComplexity": "low",
    "documentationLevel": "comprehensive"
  },
  "security": {
    "enforceInputValidation": true,
    "preventInjectionVulnerabilities": true,
    "secureDataHandling": true
  }
}
